<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kami.kami.dao.AdminMapper">

	<insert id="insertEmployee" parameterType="Employee">
		INSERT INTO EMPLOYEE
		(
			EMPLOYEEID
			,POSITIONSEQ
			,PASSWORD
			,NAME
			,BIRTHDATE
			,PHONE
			,ADDRESS
			,GENDER
		)
		VALUES
		(
			#{employeeId}
			,positionSeq.nextval
			,#{password}
			,#{name}
			,#{birthDate}
			,#{phone}
			,#{address}
			,#{gender}
		)
	</insert>
	
	
	<select id="selectEmployee" parameterType="Employee" resultType="Employee">
		SELECT
			EMPLOYEEID,
			POSITIONSEQ,
			PASSWORD,
			NAME,
			HIREDATE,
			BIRTHDATE,
			PHONE,
			ADDRESS,
			FIREDATE,
			GENDER
		FROM 
			EMPLOYEE
		WHERE
			EMPLOYEEID=#{employeeId}
			AND
			PASSWORD=#{password}
	</select>
	

</mapper>
